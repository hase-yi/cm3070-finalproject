[run]
omit =
    *__init__*
    */migrations/*
    */tests/*
    bookforest/wsgi.py
    bookforest/asgi.py
    bookforest/settings.py
    bookforest/urls.py
    core/admin.py
    core/apps.py
    core/urls.py
    core/routing.py
    core/tests.py
    manage.py

[report]
exclude_lines =
    pragma: no cover
    def __repr__
    def __str__
    if self\.debug
    if settings\.DEBUG
    raise AssertionError
    raise NotImplementedError
    if __name__ == .__main__.:
